{
  "name": "ai-browser",
  "version": "1.5.1",
  "description": "AI Browser",
  "type": "module",
  "main": "./out/main/index.js",
  "license": "MIT",
  "author": "Jun Murakami",
  "homepage": "https://github.com/Jun-Murakami/AI-Browser",
  "scripts": {
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "license-checker --production --json --out src/renderer/src/assets/license.json && npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "build-appx": "electron-builder -w appx"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/lato": "^5.2.6",
    "@fontsource/m-plus-1p": "^5.2.6",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "allotment": "^1.20.4",
    "electron-context-menu": "^4.1.1",
    "monaco-editor": "^0.52.2",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.1",
    "electron": "37.3.1",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  }
}
